{
  "name": "boutique",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev:client": "poi --serve",
    "dev:server": "nodemon -r dotenv/config ./server --ignore ./client | bunyan",
    "db": "node ./server/script/sequelize",
    "db:reset": "npm run db drop && npm run db create && npm run db migrate",
    "db:seed": "npm run db seed:all",
    "db:migration:create": "npm run db migration:create -- --name placeholder",
    "user:add": "node ./server/script/add-user.js",
    "lint:js": "eslint --ext .js,.vue .",
    "lint:md": "remark .",
    "lint:scss": "stylelint \"client/**/*.vue\" \"client/**/*.scss\"",
    "lint": "npm run lint:js; npm run lint:scss; npm run lint:md",
    "debug": "nodemon --inspect-brk -r dotenv/config ./server | bunyan",
    "readme:update": "node ./server/script/update-readme",
    "start": "NODE_ENV=production node -r dotenv/config ./server",
    "build": "del-cli node_modules/.cache/vue-loader && poi --prod",
    "webpack:inspect-config": "poi --inspect-webpack",
    "webpack:bundle-report": "poi --prod --bundle-report",
    "clean": "del-cli dist/* !dist/.gitkeep",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postshrinkwrap": "sed -i.bak 's/http:\\/\\//https:\\/\\//g' package-lock.json && rm -v package-lock.json.bak"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ExtensionEngine/boutique.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/ExtensionEngine/boutique/issues"
  },
  "homepage": "https://github.com/ExtensionEngine/boutique#readme",
  "license": "MIT",
