{
  "name": "boutique",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "start": "lerna run --parallel start",
    "build": "lerna run --parallel build",
    "dev": "lerna run --parallel dev",
    "boostrap": "npm run clean && npm run postinstall",
    "clean": "lerna clean",
    "db:migrate": "lerna run --stream db:migrate",
    "db:seed": "lerna run --stream db:seed",
    "lint:md": "remark .",
    "lint:all": "lerna run --parallel lint",
    "postinstall": "lerna bootstrap --hoist",
    "readme:update": "node ./scripts/update-readme"
  },
  "devDependencies": {
    "husky": "^4.3.8",
    "lerna": "^4.0.0",
    "read-pkg-up": "^7.0.1",
    "readme-filename": "^1.0.0",
    "remark-cli": "^9.0.0",
    "remark-lint": "^8.0.0",
    "remark-preset-lint-recommended": "^5.0.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint:all"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ExtensionEngine/boutique.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/ExtensionEngine/boutique/issues"
  },
  "homepage": "https://github.com/ExtensionEngine/boutique#readme",
  "license": "MIT",
  "engines": {
    "node": ">= 12.21.0",
    "npm": ">= 6.14.11",
    "postgres": ">= 9.4"
  }
}
