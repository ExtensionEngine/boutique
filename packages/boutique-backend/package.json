{
  "name": "boutique-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon -r dotenv/config ./src | bunyan",
    "db": "node ./src/script/sequelize",
    "db:reset": "npm run db drop && npm run db create && npm run db migrate",
    "db:seed": "npm run db seed:all",
    "db:migration:create": "npm run db migration:create -- --name placeholder",
    "user:add": "node ./src/script/add-user.js",
    "lint:js": "eslint --ext .js .",
    "lint:md": "remark .",
    "lint": "npm run lint:js; npm run lint:md",
    "debug": "nodemon --inspect-brk -r dotenv/config ./src | bunyan",
    "readme:update": "node ./src/script/update-readme",
    "prod": "NODE_ENV=production node -r dotenv/config ./src",
    "clean": "del-cli dist/* !dist/.gitkeep",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postshrinkwrap": "rewrite-lockfile package-lock.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ExtensionEngine/boutique.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/ExtensionEngine/boutique/issues"
  },
  "homepage": "https://github.com/ExtensionEngine/boutique#readme",
  "license": "MIT",
