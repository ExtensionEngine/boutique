    "node": ">= 12.21.0",
    "npm": ">= 6.14.11",
    "postgres": ">= 9.4"
  },
  "vuePlugins": {
    "service": [
      "./build/plugins/envs",
      "./build/plugins/clean-out-dir",
      "./build/plugins/html-version-spec"
