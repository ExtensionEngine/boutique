{
  "name": "boutique-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "lint:js": "eslint --ext .js,.vue .",
    "lint:scss": "stylelint \"src/**/*.vue\" \"src/**/*.scss\"",
    "lint": "npm run lint:js; npm run lint:scss",
    "build": "del-cli node_modules/.cache/vue-loader && vue-cli-service build",
    "webpack:bundle-report": "vue-cli-service build --report",
    "webpack:inspect-config": "vue-cli-service inspect",
    "clean": "del-cli dist/* !dist/.gitkeep",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postshrinkwrap": "rewrite-lockfile package-lock.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ExtensionEngine/boutique.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/ExtensionEngine/boutique/issues"
  },
  "homepage": "https://github.com/ExtensionEngine/boutique#readme",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@extensionengine/eslint-config": "^2.0.0",
    "@extensionengine/stylelint-config": "^1.1.1",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "del-cli": "^3.0.1",
    "emoji-strip": "^1.0.1",
    "esbuild-loader": "^2.10.0",
    "eslint": "^7.22.0",
    "eslint-config-semistandard": "^15.0.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-require-sort": "^1.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^7.7.0",
    "eslint-plugin-vuetify": "^1.0.0-beta.6",
    "node-sass": "^5.0.0",
    "rewrite-lockfile": "^1.0.1",
    "sass": "^1.26.8",
    "sass-loader": "^10.1.1",
    "stylelint": "^13.0.0",
    "vue-template-compiler": "^2.6.11",
    "webpack-bundle-analyzer": "^4.0.0"
  },
  "dependencies": {
    "@mdi/js": "^5.9.55",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "cuid": "^2.1.8",
    "date-fns": "^2.14.0",
    "dotenv": "^8.2.0",
    "fuzzysearch": "^1.0.3",
    "humanize-string": "^2.1.0",
    "lodash": "^4.17.20",
    "ms": "^2.1.3",
    "promise-queue": "^2.2.5",
    "sass-web-fonts": "^2.1.0",
    "save-as": "^0.1.8",
    "semver": "^7.3.4",
    "tailor-teaching-elements": "^1.5.0",
    "truncate": "^2.1.0",
    "url-join": "^4.0.1",
    "v-hotkey": "^0.9.0",
    "vee-validate": "^3.3.7",
    "vue": "^2.6.11",
    "vue-router": "^3.3.4",
    "vue-timeago": "^5.1.3",
    "vue-visible": "^1.0.2",
    "vuetify": "^2.3.4",
    "vuex": "^3.4.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
